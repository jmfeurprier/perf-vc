{
	"name"        : "perf/vc",
	"type"        : "library",
	"license"     : "MIT",
	"description" : "Simple View-Controller stack",
	"require"     : {
		"php"                          : ">=7.4",
        "ext-json"                     : "*",
      	"ext-simplexml"                : "*",
		"perf/http-status"             : "^3.0",
		"perf/source"                  : "^2.0",
		"perf/caching"                 : "^3.1",
		"perf/timing"                  : "^2.0",
		"twig/twig"                    : "^3.0",
		"symfony/dependency-injection" : "^5.0",
		"symfony/expression-language"  : "^5.1",
		"symfony/yaml"                 : "^5.0"
    },
	"require-dev": {
		"ext-xdebug"                : "*",
		"phing/phing"               : "^2.16",
		"phpmd/phpmd"               : "^2.8",
		"phpunit/phpunit"           : "^9.3",
		"squizlabs/php_codesniffer" : "^3.5"
	},
	"autoload" : {
		"psr-4" : {
			"perf\\Vc\\" : "src/"
		}
